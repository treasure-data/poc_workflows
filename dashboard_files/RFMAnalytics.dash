  "script": "// Define custom theme variables to be used in styling syntax across the Dash\nvar dashboardBackgroundColor = '#f2f9ff'; //#9E6762\nvar widgetBackgroundColor = '#ffffff'; \nvar parentBackgroundColor = '#f2f9ff';\nvar borderRadius = '20px';\nvar fontFamily = 'Nunito, sans-serif';\nvar fontStyle = 'bold';\nvar eastHandleWidth = '0.5px';\nvar southHandleHeight = '0.5px';\nvar subCellVerticalBackgroundColor = 'white';\nvar subCellVerticalBoxShadow = '4px 5px 12px #00000078';\nvar subCellHostPadding = '10px';\nvar layoutPaddingRight = '10px';\nvar layoutPaddingLeft = '10px';\nvar IndicatorPrimaryColor = '#58799c';\n//var TableHeaderColor = '#D3EBF7'\nvar TableHeaderColor = '#e5e7e9';\n\n\n\ndashboard.on('widgetready', function(d) {\n\t\n\t$('.sis-scope .uc-tagline-content .uc-tag').css({'background-color' : 'rgb(243, 238, 255) !important'}); \n\t$('.sis-scope .uc-tagline-content .uc-tag').css({'color' : 'rgb(130, 75, 251) !important '}); \n\t$('.sis-scope .btn--new-widget').css({'background-color' : 'rgb(243, 238, 255) !important'}); \n\t$('.sis-scope .btn--new-widget').css({'color' : 'rgb(130, 75, 251) !important'}); \n\t\n\t//Below apply Styling changes at the Global Dashboard Level across individual widgets and DOM objects\n\t$('.table-grid__row-0 td.table-grid__cell--row-0').css({'background-color' : TableHeaderColor,'border':'0px','font-weight':'500 !important'}); \t//Controls Header Colors of Pivot Tables\n    $('.dashboard-layout-cell-horizontal-divider').css('display', 'none');\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t//Makes horizonatal divider invisible\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\n    $('label.transput-input-placeholder').css('display', 'none');\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t//Makes 'LABEL HERE' Widget title palceholder invisible\n\t\n\t$('.dashboard-layout-column').css('background-color', dashboardBackgroundColor);\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t//Controls background color of columns/containers in Dash\n    $('.dashboard-layout').css('background-color', dashboardBackgroundColor);\t\t\t\t\t\t\t\t\t\t\t\t\t//Controls Dashboard Main Background Color\n    $('.dashboard-layout-subcell-vertical').css('background-color','white').css('box-shadow', subCellVerticalBoxShadow);\t\t//Controls background and shadow color and size\n    $('.dashboard-layout-subcell-host').css('padding', subCellHostPadding);\t\t\t\t\t\t\t\t\t\t\t\t\t\t//Controls padding\n    $('.dashboard-layout').css('padding-right', layoutPaddingRight).css('padding-left', layoutPaddingLeft);\n\t\n    $('.ui-resizable-handle.ui-resizable-e').css('width', eastHandleWidth);\t\t\t\t\t\t\t\t\t\t\t\t\t\t//Controls how Dash looks when resized in the Browser Width\n    $('.ui-resizable-handle.ui-resizable-s').css('height', southHandleHeight);\t\t\t\t\t\t\t\t\t\t\t\t\t//Controls how Dash looks when resized in the Browser Height\n\t\n\tvar optionsBar = {\n\t\tneedleColor: '#5d8ba0',  //Gauge Only\n\t\ttickColor: '#5d8ba0',\t  //Gauge Only\n\t\t//bracketColor: '#D3EBF7', //Box and Ticker\n\t\tbracketColor: '#5d8ba0', //Box and Ticker\n\n\t\ttitle: {\n\t\t\tfontFamily: 'Nunito, sans-serif',\n\t\t\tfontWeight: 500,\n\t\t\tfontSizes: {\n\t\t\t\tbig: 25,\n\t\t\t\tmedium: 20\n\t\t\t}\n\t\t},\n\t\tvalue: {\n\t\t\tfontFamily: 'Nunito, sans-serif',\n\t\t\tfontWeight: 'bold',\n\t\t\tcolor: '#58799c',\n\t\t\tfontSizes: {\n\t\t\t\tbig: 50,\n\t\t\t\tmedium: 35\n\t\t\t}\n\t\t},\n\t\tsecondaryTitle: {\n\t\t\tfontFamily: 'Nunito, sans-serif',\n\t\t\tfontWeight: 300,\n\t\t\tfontSizes: {\n\t\t\t\tbig: 20,\n\t\t\t\tmedium: 15\n\t\t\t}\n\t\t},\n\t\tsecondaryValue: {\n\t\t\tfontFamily: 'Nunito, sans-serif',\n\t\t\tcolor: '#8d4e3c',\n\t\t\tfontSizes: {\n\t\t\t\tbig: 20,\n\t\t\t\tmedium: 15\n\t\t\t}\n\t\t},\n\t\tlabel: {\n\t\t\tfontFamily: 'Nunito, sans-serif',\n\t\t\tcolor: '#77A6BB',\n\t\t\tfontSizes: {\n\t\t\t\tbig: 20,\n\t\t\t\tmedium: 15\n\t\t\t}\n\t\t}\n\n\n\t};\n\n\t\n\t\n    $('.widget').each(function() {\n        $(this).css({\n            'background-color': widgetBackgroundColor,\n            'border-radius': borderRadius\n        });\n        $(this).parent().css({\n            'background-color': parentBackgroundColor,\n            'border-radius': borderRadius,\n            'font-family': fontFamily,\n            'font-style': fontStyle\n        });\n    });\n\t\n\n\t\n\n});\n\n\n\n\n\n// Function to apply color palette to widget\nfunction applyColorPalette(widget) {\n  widget.on('processresult', function(se, ev) {\n     \n\tif (widget.indicatorInstance) {\n      var options = {\n\t\t  needleColor: '#5d8ba0',  //Gauge Only\n\t\t  tickColor: '#5d8ba0',\t  //Gauge Only\n\t\t  bracketColor: '#D3EBF7', //Box and Ticker\n\t\t  \n\t\t  title: {\n\t\t\t  fontFamily: 'Nunito, sans-serif',\n\t\t\t  fontWeight: 500,\n\t\t\t  fontSizes: {\n\t\t\t\t  big: 25,\n\t\t\t\t  medium: 20\n\t\t\t  }\n\t\t  },\n\t\t  value: {\n\t\t\t  fontFamily: 'Nunito, sans-serif',\n\t\t\t  fontWeight: 'bold',\n\t\t\t  color: '#58799c',\n\t\t\t  fontSizes: {\n\t\t\t\t  big: 50,\n\t\t\t\t  medium: 35\n\t\t\t  }\n\t\t  },\n\t\t  secondaryTitle: {\n\t\t\t  fontFamily: 'Nunito, sans-serif',\n\t\t\t  fontWeight: 300,\n\t\t\t  fontSizes: {\n\t\t\t\t  big: 20,\n\t\t\t\t  medium: 15\n\t\t\t  }\n\t\t  },\n\t\t  secondaryValue: {\n\t\t\t  fontFamily: 'Nunito, sans-serif',\n\t\t\t  color: '#824bfb',\n\t\t\t  fontSizes: {\n\t\t\t\t  big: 20,\n\t\t\t\t  medium: 15\n\t\t\t  }\n\t\t  },\n\t\t  label: {\n\t\t\t  fontFamily: 'Nunito, sans-serif',\n\t\t\t  color: '#77A6BB',\n\t\t\t  fontSizes: {\n\t\t\t\t  big: 20,\n\t\t\t\t  medium: 15\n\t\t\t  }\n\t\t  }\n\n\t\t  \t  \n\t  }\n\n      widget.indicatorInstance.setOptions('numericSimple', options);\n      widget.indicatorInstance.setOptions('numericBar', optionsBar);\n  \t  widget.indicatorInstance.setOptions('ticker', options);\n  \t  widget.indicatorInstance.setOptions('gauge', options);\n    }\n  });\n}\n\n// Apply color palette to all widgets in the dashboard\ndashboard.on('widgetinitialized', function(se, ev) {\n  applyColorPalette(ev.widget);\n});\n\n// Apply color palette to all existing widgets in the dashboard\ndashboard.widgets().forEach(function(widget) {\n  applyColorPalette(widget);\n});\n\n\n\n",
