      "script": "widget.on('ready', function(w, args) {\n    // Access query result\n    var data = w.queryResult;\n\n    // Initialize an array to hold the formatted data\n    var chartData = [];\n\n    // Object to dynamically assign colors to tiers\n    var groupColors = {};\n    var colorPalette = ['#58799c', '#8da6bf', '#BCC7D3', '#b7aad3', '#9f95b5', '#77A6BB','#BEDFEE','#578076','#73A095', '#A6D1C7','#9A7D36', '#BB9E54', '#E7D096', '#9E6762', '#C18983','#5D8BA0', '#91BDD1', '#DAA7A3'];\n    var colorIndex = 0;\n\n    // Variables to store axis titles (from the Panel names)\n    var xAxisTitle, yAxisTitle, zAxisTitle;\n\n    // Loop over the data to extract values and panel names\n    for (var i = 0; i < data.length; i++) {\n        var row = data[i];\n\n        // Extract axis values from the 'Value' field\n        var x = row[\"1\"].Value;  // X-axis (e.g. avg_frequency)\n        var y = row[\"2\"].Value;  // Y-axis (e.g. avg_monetary)\n        var z = row[\"3\"].Value;  // Z-axis (e.g. avg_recency)\n        var tier = row[\"0\"].Value;  // Tier (used for color assignment)\n\n        // Set axis titles based on the 'Panel' field (assuming it's the same for all rows)\n        if (!xAxisTitle) xAxisTitle = row[\"1\"].Panel;\n        if (!yAxisTitle) yAxisTitle = row[\"2\"].Panel;\n        if (!zAxisTitle) zAxisTitle = row[\"3\"].Panel;\n\n        // Dynamically assign colors to each tier if not already assigned\n        if (!groupColors[tier]) {\n            groupColors[tier] = colorPalette[colorIndex] || 'rgba(0,0,0,0.5)';  // Fallback color if palette runs out\n            colorIndex++;\n        }\n\n        // Push the data in the format required by your chart\n        chartData.push({ x: x, y: y, z: z, tier: tier });\n    }\n\n    // Use chartData in a custom function or pass it to a chart\n    createChart(chartData, groupColors, xAxisTitle, yAxisTitle, zAxisTitle);\n});\n\n\nfunction createChart(chartData, groupColors, xAxisTitle, yAxisTitle, zAxisTitle) {\n\tvar script = document.createElement('script');\n\tscript.src = 'https://cdn.plot.ly/plotly-latest.min.js';\n\t//script.onload = callback;\n\tscript.onerror = function() {\n\t\tconsole.error('Failed to load Plotly.js script.');\n\t};\n\tdocument.head.appendChild(script);\n\n    // Group data by tier\n    var groupedData = {};\n    chartData.forEach(function(point) {\n        if (!groupedData[point.tier]) {\n            groupedData[point.tier] = [];\n        }\n        groupedData[point.tier].push(point);\n    });\n\n    // Create a trace for each tier\n    var traces = Object.keys(groupedData).map(function(tier) {\n        var points = groupedData[tier];\n        return {\n            x: points.map(point => point.x),\n            y: points.map(point => point.y),\n            z: points.map(point => point.z),\n            mode: 'markers',\n            type: 'scatter3d',\n            marker: {\n                size: 5,\n                color: groupColors[tier],  // Apply the color for each tier\n            },\n            name: tier,  // Use the tier as the trace name to display in the legend\n            text: points.map(point => point.tier),\n            hoverinfo: 'text'\n        };\n    });\n\n    // Define layout\n    var layout = {\n        scene: {\n            xaxis: { title: xAxisTitle || 'X-Axis' },\n            yaxis: { title: yAxisTitle || 'Y-Axis' },\n            zaxis: { title: zAxisTitle || 'Z-Axis' }\n        },\n        title: '3D Scatter Plot with Tier-based Colors',\n        margin: { l: 0, r: 0, b: 0, t: 50 },\n        showlegend: true  // Enable the legend\n    };\n\n    // Create the plot\n    Plotly.newPlot('3dplotjsnew', traces, layout);\n}\n\n// Ensure chart persists by attaching to widget-level events\nwidget.on('ready', function() {\n    //loadHighcharts(function() {\n        // Do nothing here, as the chart will be created when data is available\n   // });\n});\n\n// Redraw chart when the widget is resized or re-rendered\nwidget.on('render', function() {\n    // Do nothing here, as the chart will be redrawn when data is available\n});\n"
